import numpy as np
p,r=map(int,input().split())
l1=list(map(int,input().split()))
l2=list(map(int,input().split()))
m1=np.array(l1).reshape(p,r)
m2=np.array(l2).reshape(p,r)
o1=np.add(m1,m2)
o2=np.subtract(m1,m2)
o3=np.multiply(m1,m2)
o4=np.divide(m1,m2)
print(f'after addition {o1}')
print(f'after asubtraction {o2}')
print(f'after multiplication {o3}')
print(f'after division {o4}')
